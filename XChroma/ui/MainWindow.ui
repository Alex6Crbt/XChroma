    background-color: #444444;
    border: 1px solid #65DBF9; /* Blue border on hover */
}

QPushButton:pressed {
    background-color: #222222;
}

/* Labels */
QLabel {
    color: #E0E0E0;
}

/* ComboBox */
QComboBox, QSpinBox {
    background-color: #1E1E1E;
